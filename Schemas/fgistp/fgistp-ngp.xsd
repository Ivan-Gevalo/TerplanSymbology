<?xml version="1.0" encoding="utf-8"?>

<!-- 
    Приведенная схема является модификацией XML-схемы, являющейся приложением к приказу Министерства экономического развития Российской Федерации от 13.02.2025 № 89 и используемой для подготовки электронных XML-документов, воспроизводящих сведения из основной части нормативов градостроительного проектирования, впоследствии размещаемых на официальном сайте Федеральной государственной информацинной системы территориального планирования (ФГИС ТП).
	
	Файл оригинальной схемы размещен на сайте ФГИС ТП: https://fgistp.economy.gov.ru/xsd/fgistp-ngp_4.xsd

    В данной модификации внесены следующие изменения:
    - из описания составных типов удален элемент ZNP_TYPE; в оригинальной схеме для данного элемента указан одноименный тип, однако определение данного типа в XML-схеме отсутствует;
    - удалены неиспользуемые справочники;
    - описания кодов справочников перенесены из атрибута fgistp:username в аннотации;
	- удалены атрибуты fgistp:username, поскольку значения данных атрибутов дублируются в аннотациях.

    Файл модифицированной схемы размещен на сайте Terplan Symbology по ссылке https://sites.google.com/view/terplan-symbology/fgistp
-->

<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
	xmlns:gml="http://www.opengis.net/gml/3.2"
	xmlns:fgistp="http://fgistp"
	targetNamespace="http://fgistp"
	elementFormDefault="qualified"
	attributeFormDefault="unqualified">
	<xs:import
		namespace="http://www.opengis.net/gml/3.2"
		schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd"/>

	<!-- Описание простых типов -->
	<xs:simpleType name="UUID">
		<xs:restriction base="xs:string">
			<xs:pattern value="(urn:uuid:)?[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|\{[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="OKTMO">
		<xs:annotation>
			<xs:documentation>Код из Общероссийского классификатора территорий муниципальных образований</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="8"/>
			<xs:maxLength value="11"/>
			<xs:pattern value="[0-9]{8}"/>
			<xs:pattern value="[0-9]{11}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Описание составных типов -->
	<xs:complexType name="Education_Type">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="CLASSID" type="fgistp:Education_CLASSID">
						<xs:annotation>
							<xs:documentation>Код объекта</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OKTMO" type="fgistp:OKTMO">
						<xs:annotation>
							<xs:documentation>Код ОКТМО</xs:documentation>
						</xs:annotation>
					</xs:element>
						<xs:element name="EDU_TYPE" type="fgistp:EDU_TYPE" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Перечень возможных объектов</xs:documentation>
						</xs:annotation>
					</xs:element>
						<xs:element name="EYR_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности местами на 100 человек, окончивших обучение по программам основного общего и среднего общего образования</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ASLI_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, число мест на 100 детей в возрасте от 0 до 7 лет</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ASLIT_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, мест для детей в возрасте 0-7 лет на 1000 жителей</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SCOL_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, число мест на 100 детей в возрасте от 7 до 18 лет</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SCOLI_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, число мест на 100 детей в возрасте от 5 до 18 лет</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PSI_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, центр на 5 тыс. детского населения</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="INTNT_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, 1 на 10 тыс. детского населения, но не менее 1 в субъекте РФ</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EUR_OBJ" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, мест в образовательной организации для учащихся очной формы</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MXDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Максимальная доступность, пешеходная доступность, метры, (минуты)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TR_DOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Транспортная доступность, мин.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RZUDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Размер земельного участка, кв. м на 1 место</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="POPULATIONF" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Численность населения, тыс. чел.(Факт)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="POPULATIONP" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Численность населения, тыс. чел.(План)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Education" type="fgistp:Education_Type" substitutionGroup="gml:AbstractFeature">
		<xs:annotation>
			<xs:documentation>Класс объектов «Объекты образования и науки»</xs:documentation>
		</xs:annotation>
	</xs:element>
	
	<xs:complexType name="Culture_Type">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="CLASSID" type="fgistp:Culture_CLASSID">
						<xs:annotation>
							<xs:documentation>Код объекта</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OKTMO" type="fgistp:OKTMO">
						<xs:annotation>
							<xs:documentation>Код ОКТМО</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CUL_TYPE" type="fgistp:CUL_TYPE" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Перечень возможных объектов</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULB_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения региональными и межмуниципальными библиотеками, количество объектов на 10 000 чел.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULSB_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, количество библиотек каждого вида на субъект РФ</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULONE_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения музеями, количество на 1 млн. чел</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULf_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения музеями, количество на 500 тыс. - 1 млн. чел</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULGOR_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения музеями, количество на 150 тыс.- 500 тыс. чел</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULONET_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения театрами, количество на 1 млн. чел</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULfT_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения театрами, количество на 500 тыс. - 1 млн. чел</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULGORT_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения театрами, количество на 150 тыс.-500 тыс. чел</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULGORK_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения концертными залами, количество от 500 тыс. чел.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULONETK_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения концертными залами, количество до 500 тыс. чел.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULfTC_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения цирковыми аттракционами/площадками, количество от 1 млн. чел.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULDK_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности населения дворцами культуры</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULGORZO_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, на 500 тыс. населения</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CULONZUU_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, на 250 тыс. населения</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MXDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Максимальная доступность, пешеходная доступность, метры, минуты</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TR_DOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Транспортная доступность, мин.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RZUDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Размер земельного участка, кв. м на 1 место</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RZUGDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Размер земельного участка, га</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="POPULATIONF" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Численность населения, тыс. чел.(Факт)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="POPULATIONP" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Численность населения, тыс. чел.(План)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Culture" type="fgistp:Culture_Type" substitutionGroup="gml:AbstractFeature">
		<xs:annotation>
			<xs:documentation>Класс объектов «Объекты культуры и искусства»</xs:documentation>
		</xs:annotation>
	</xs:element>
	
	<xs:complexType name="Sport_Type">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="CLASSID" type="fgistp:Sport_CLASSID">
						<xs:annotation>
							<xs:documentation>Код объекта</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OKTMO" type="fgistp:OKTMO">
						<xs:annotation>
							<xs:documentation>Код ОКТМО</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SP_TYPE" type="fgistp:SP_TYPE" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Перечень возможных объектов</xs:documentation>
						</xs:annotation>
					</xs:element>		
					<xs:element name="SP_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, единиц на 100 000 жителей</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SPM_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, м2 на 10000 жителей</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MXDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Максимальная доступность, пешеходная доступность, метры, (минуты)</xs:documentation>
						</xs:annotation>
					</xs:element>					
					<xs:element name="TR_DOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Транспортная доступность, мин.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RZUDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Размер земельного участка, кв. м на 1 место</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RZUGDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Размер земельного участка, га</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="POPULATIONF" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Численность населения, тыс. чел.(Факт)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="POPULATIONP" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Численность населения, тыс. чел.(План)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Sport" type="fgistp:Sport_Type" substitutionGroup="gml:AbstractFeature">
		<xs:annotation>
			<xs:documentation>Класс объектов «Объекты физкультурного и спортивного назначения»</xs:documentation>
		</xs:annotation>
	</xs:element>
	
	<xs:complexType name="Health_Type">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="CLASSID" type="fgistp:Health_CLASSID">
						<xs:annotation>
							<xs:documentation>Код объекта</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OKTMO" type="fgistp:OKTMO">
						<xs:annotation>
							<xs:documentation>Код ОКТМО</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HEA_TYPE" type="fgistp:HEA_TYPE" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Перечень возможных объектов</xs:documentation>
						</xs:annotation>
					</xs:element>		
					<xs:element name="HEA_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, единиц на 50000 жителей</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NEOT_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, единиц на 20000–50000 жителей</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AMB_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, посещений на 1000 жителей/детей/взрослых</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HEASB_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, объектов на субъект РФ</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HEAONE_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, численность населения, тыс. чел на 1 объект</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HEADET_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, мест на детей 3 - 18 лет на 1000 жителей</xs:documentation>
						</xs:annotation>
					</xs:element>					
					<xs:element name="HEAGOR_OBES" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Уровень обеспеченности, единиц на муниципальное образование, крупный, крупнейший город</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MXDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Максимальная доступность, пешеходная доступность, метры, минуты</xs:documentation>
						</xs:annotation>
					</xs:element>					
					<xs:element name="TR_DOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Транспортная доступность, мин.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RZUDOS" type="xs:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Размер земельного участка, кв. м на 1 место</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="POPULATIONF" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Численность населения, тыс. чел.(Факт)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="POPULATIONP" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Численность населения, тыс. чел.(План)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Health" type="fgistp:Health_Type" substitutionGroup="gml:AbstractFeature">
		<xs:annotation>
			<xs:documentation>Класс объектов «Объекты здравоохранения»</xs:documentation>
		</xs:annotation>
	</xs:element>
	
	<xs:complexType name="Social_Type">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
